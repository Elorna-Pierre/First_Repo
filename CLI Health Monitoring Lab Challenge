print("Maximum of heart rate in list is")
print(max(heart_rate))


print("Minimum of heart rate in list is")
print(min(heart_rate))


print("Average heart rate of list is")
heart_rate = [85,74,79,81,75,77,83,76]
print((sum(heart_rate))/(len(heart_rate)))


print("Standard deviation of heart rate of list is")
heart_rate = [85,74,79,81,75,77,83,76]
print(statistics.stdev((heart_rate)))


